---
layout: archive
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Linh Ho's Research</title>
	<link href="http://linhho.github.io/research/atom.xml" rel="self"/>
	<link href="http://linhho.github.io/research"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://linhho.github.io/research</id>
	<author>
		<name>Linh Ho</name>
		<email>linhho@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://linhho.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://linhho.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
